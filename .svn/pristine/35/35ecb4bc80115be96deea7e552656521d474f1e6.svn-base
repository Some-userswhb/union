<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <title>添智·智慧工会云服务平台</title>

    <link rel="shortcut icon" href="favicon.ico">
    <link href="css/bootstrap.min.css?v=3.3.6" rel="stylesheet">
    <link href="css/font-awesome.css?v=4.4.0" rel="stylesheet">
    <link href="css/animate.css" rel="stylesheet">
    <link href="css/style.css?v=4.1.0" rel="stylesheet">

</head>
<style>
    .col-sm-8 input.form-control, .col-sm-8 select.form-control {
        width: 50%;
        display: inline-block;
    }

    .help-block.m-b-none {
        display: inline;
        margin-left: 10px;
    }

    .form-group {
        margin-bottom: 20px;
    }

    .juzhong th,.juzhong2 td{
        text-align: center;
    }
</style>
<body class="gray-bg">
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <form class="form-horizontal m-t" id="AddQualification">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">*&nbsp;所属公会：</label>
                        <div class="col-sm-8">
                            <select class="form-control" id='area' name="area">
                                <option>山西省总工会</option>
                                <option>选项 2</option>
                                <option>选项 3</option>
                                <option>选项 4</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">*&nbsp;统一社会信用代码：</label>
                        <div class="col-sm-8">
                            <input id="lastname" name="lastname" class="form-control" type="text" aria-required="true"
                                   aria-invalid="false" class="valid">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">*&nbsp;工会法人：</label>
                        <div class="col-sm-8">
                            <select class="form-control" id='area' name="area">
                                <option>太原市总工会</option>
                                <option>选项 2</option>
                                <option>选项 3</option>
                                <option>选项 4</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group juzhong">
                        <label class="col-sm-3 control-label">法人信息：</label>
                        <div class="col-sm-8">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>会员姓名</th>
                                        <th>会员证号</th>
                                        <th>证件号</th>
                                        <th>公会职务</th>
                                        <!-- <th>操作</th> -->
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td> </td>
                                        <td> </td>
                                        <td> </td>
                                        <td style="text-align:right;">
                                            <span class="glyphicon glyphicon-ok" style="margin-right: 10px;"></span>
                                        </td>
                                        <!-- <td>
                                            <i class="fa fa-trash"></i>
                                        </td> -->
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="form-group juzhong juzhong2">
                        <label class="col-sm-3 control-label">收入信息：</label>
                        <div class="col-sm-8">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>上年结余累计</th>
                                        <th>年会员会费</th>
                                        <th>年2%经费流程</th>
                                        <th>其他收入</th>
                                        <!-- <th>操作</th> -->
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><input type="text" placeholder="元"></td>
                                        <td><input type="text" placeholder="元"></td>
                                        <td><input type="text" placeholder="元"></td>
                                        <td><input type="text" placeholder="元"></td>
                                       <!--  <td>
                                            <i class="fa fa-trash"></i>
                                        </td> -->
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="form-group juzhong juzhong2">
                        <label class="col-sm-3 control-label">资金情况：</label>
                        <div class="col-sm-8">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>合计</th>
                                        <th>固定资金</th>
                                        <th>流动资金</th>
                                        <th>其他</th>
                                        <!-- <th>操作</th> -->
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            <span style="display: block;width: 160px;">自动计算</span>
                                        </td>
                                        <td><input type="text" placeholder="元"></td>
                                        <td><input type="text" placeholder="元"></td>
                                        <td><input type="text" placeholder="元"></td>
                                        <!-- <td>
                                            <i class="fa fa-trash"></i>
                                        </td> -->
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="form-group juzhong juzhong2">
                        <label class="col-sm-3 control-label">场所情况：</label>
                        <div class="col-sm-8">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>合计</th>
                                        <th>办公场所</th>
                                        <th>活动场所</th>
                                        <th>其他场所</th>
                                        <!-- <th>操作</th> -->
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            <span style="display: block;width: 160px;">自动计算</span>
                                        </td>
                                        <td><input type="text" placeholder="平方米"></td>
                                        <td><input type="text" placeholder="平方米"></td>
                                        <td><input type="text" placeholder="平方米"></td>
                                        <!-- <td>
                                            <i class="fa fa-trash"></i>
                                        </td> -->
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">民事责任能力：</label>
                        <div class="col-sm-8">
                            <textarea rows="3" cols="50" style="resize: none;">最多输入500字</textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-8 col-sm-offset-3">
                            <input type="text" class="btn btn-primary"
                                   value="保存" style="width: 60px" @click="addZiGe">


                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- 全局js -->
<script src="js/jquery.min.js?v=2.1.4"></script>
<script src="js/bootstrap.min.js?v=3.3.6"></script>

<script src="js/vue2.5.22.js"></script>
<script src="js/ixios.0.18.0.js"></script>
<script src="https://cdn.bootcss.com/qs/6.6.0/qs.min.js"></script>

<script>
    new Vue({
        el:'#AddQualification',
        data:{
            laborunCode:'',
            socialCreditCode:'',
            dutyType:'',
            memNumber:'',
            lastYearBalance:'',
            annualMemsFee:'',
            annualFunds:'',
            otherIncome:'',
            fixedCapital:'',
            workingCapital:'',
            otherCapital:'',
            office:'',
            activities:'',
            otherPlace:'',
            civilLiability:'',
        },
        methods:{
            addZiGe:function () {
                axios({
                    method: 'post',
                    headers: {'Content-Type': 'application/x-www-form-urlencoded'},
                    url: "http://112.35.92.177:90/passLaborweb/api/laborun/laborunLegal/add",
                    data: Qs.stringify({
                        "laborunCode":this.laborunCode,
                        "socialCreditCode":this.socialCreditCode,
                        "dutyType":this.dutyType,
                        "memNumber":this.memNumber,
                        "lastYearBalance":this.lastYearBalance,
                        "annualMemsFee":this.annualMemsFee,
                        "annualFunds":this.annualFunds,
                        "otherIncome":this.otherIncome,
                        "fixedCapital":this.fixedCapital,
                        "workingCapital":this.workingCapital,
                        "otherCapital":this.otherCapital,
                        "office":this.office,
                        "activities":this.activities,
                        "otherPlace":this.otherPlace,
                        "civilLiability":this.civilLiability,
                        }),
                }).then((res) => {
                    console.log(res.data);


                });
            }
        }
    })
</script>
<!-- 自定义js -->
<script src="js/content.js?v=1.0.0"></script>

<!-- jQuery Validation plugin javascript-->
<script src="js/plugins/validate/jquery.validate.min.js"></script>
<script src="js/plugins/validate/messages_zh.min.js"></script>

<script src="js/demo/form-validate-demo.js"></script>

</body>

</html>
