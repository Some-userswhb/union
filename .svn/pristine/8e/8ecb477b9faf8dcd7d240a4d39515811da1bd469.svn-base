<style>
    .col-sm-8 input.form-control, .col-sm-8 select.form-control {
        width: 50%;
        display: inline-block;
    }

    .help-block.m-b-none {
        display: inline;
        margin-left: 10px;
    }

    .form-group {
        margin-bottom: 20px;
    }

    .uoloadfile {
        width: 200px;
        height: 100px;
        position: relative;
        margin: .2rem;
        /*border:1px solid #000;*/
        overflow: hidden;
    }

    .uoloadfile img {
        position: absolute;
        top: 0rem;
        z-index: 9998;
    }

    .change {
        opacity: 0;
        width: 100%;
        height: 100%;
        margin: 0;
        padding: 0;
    }
    .completed span:first-child,.proceeding span:first-child,.undone span:first-child{
        display: inline-block;
        width: 20px;
        height: 20px;
        margin-right: 5px;
    }
    .completed,.proceeding,.undone{
        width: 90px;
    }
    .completed span:first-child{
        background: #bbb;
    }
    .proceeding span:first-child{
        background:rgba(139, 195, 74, 0.58);
    }
    .undone span:first-child{
        background:rgba(255, 152, 0, 0.64);
    }

    .completed span:last-child,.proceeding span:last-child,.undone span:last-child{
        display: inline-block;
        width: 50px;
        height: 20px;
        margin-bottom: 10px;
        vertical-align:middle;
    }
    .completed span:last-child{
        color: #bbb;
    }
    .proceeding span:last-child{
        color:rgba(139, 195, 74, 0.58);
    }
    .undone span:last-child{
        color:rgba(255, 152, 0, 0.64);
    }
    .col-sm-12 .process-list{
        margin-left: -20px;
        width: 140px;
        float: left;

    }
    .col-sm-12 .process-list li{
        text-align: center;
        line-height: 43.5px;
    }
    .process-list li:nth-child(odd){
        width: 140px;
        height: 43.5px;
        color: #000;
        background: aquamarine;
    }
    .process-list li:nth-child(even){
        width: 1px;
        height: 43px;
        margin: 0  70px;
        background-color:#bbb ;
    }

    .col-sm-12 .audit-results{
        float: left;
        margin-left: 50px;
        font-size: 10px;

    }
    .col-sm-12 .audit-results li{
        margin-bottom: 3px;
    }
    .nav>li>a:focus, .nav>li>a:hover{
        background: none;
    }
    .nav > li > a{
        color: #333;
    }
    .nav > li.active{
        background: none;
    }
</style>


