<script>
    var vm = new Vue({
        el: "#indexVue",
        data: {
            messages: [],
            todoInfos:[],
            permsMenus:[],

        },
        mounted() {
            var that = this;
            that.getMessage();
            that.getTodoInfo(),
            that.getPermMenus()



        },
        methods: {
            getMessage(){
                var that = this
                axios({
                    method: 'post',
                    headers: {'Content-Type': 'application/json'},
                    url: baseUrl + "/passLaborweb/api/laborun/message/getHomeMessagePage",
                    data: JSON.stringify({
                        "size": 5,
                        "page": 1
                    })
                }).then(function (res) {
                    if(res.data.code === '0'){
                        if(res.data.data.content.length >0){
                            that.messages = res.data.data.content;
                        }
                    }
                });
            },
            getMoreMessage(){
                layer.open({
                    type: 2,
                    title: '',
                    shadeClose: true,
                    skin: 'layui-layer-molv',
                    area: ['50%', '80%'],
                    content: 'index_more_message.html'
                })
            },
            getTodoInfo(){
                var that = this
                axios({
                    method: 'post',
                    headers: {'Content-Type': 'application/json'},
                    url: baseUrl + "/passLaborweb/api/laborun/laborunTodoInfo/queryPage",
                    data: JSON.stringify({
                        "size": 5,
                        "page": 1
                    })
                }).then(function (res) {
                    if(res.data.code === '0'){
                        if(res.data.data.content.length >0){
                            that.todoInfos = res.data.data.content;
                        }
                    }
                });
            },
            getMoretodoInfo(){
                layer.open({
                    type: 2,
                    title: '',
                    shadeClose: true,
                    skin: 'layui-layer-molv',
                    area: ['50%', '80%'],
                    content: 'index_more_todo.html'
                })
            },
            getPermMenus(){
                var that = this
                axios({
                    method: 'post',
                    headers: {'Content-Type': 'application/x-www-form-urlencoded'},
                    url: userUrl + "/sso/ssoRole/getRoleResouce",
                    data: Qs.stringify({
                        "systemCode": "S1006",
                        "roleId":"1066511362700689408"
                    })
                }).then(function (res) {
                    console.log(res.data.data);
                    console.log(res.data);
                    console.log(res);
                    if(res.data.code === '0'){
                        if(res.data.data.length >0){
                            that.permsMenus = res.data.data;
                            console.log(that.permsMenus)
                            if(that.permsMenus.length > 0){
                                for(var i=0;i<that.permsMenus.length;i++){
                                    var item = that.permsMenus[i];
                                    if(item.name === '数据统计中心'){
                                        document.getElementById("dataManager").style.display = "inline";
                                    }
                                    if(item.name === '通知管理'){
                                        document.getElementById("noticeManager").style.display = "inline";

                                    }
                                    if(item.name === '用户实名管理'){
                                        document.getElementById("realNameManager").style.display = "inline";
                                    }
                                    if(item.name === '组织管理'){
                                        document.getElementById("organManager").style.display = "inline";
                                        document.getElementById("organManager").setAttribute("code",item.code);
                                        console.log(item.code)
                                    }
                                    if(item.name === '企事业管理'){
                                        document.getElementById("unitManager").style.display = "inline";
                                    }
                                    if(item.name === '会员管理'){
                                        document.getElementById("memManager").style.display = "inline";
                                    }
                                    if(item.name === '工会人事管理'){
                                        document.getElementById("personManager").style.display = "inline";
                                    }
                                    if(item.name === '平台内容管理'){
                                        document.getElementById("platManager").style.display = "inline";
                                    }

                                }
                            }

                        }
                    }
                });
            },

        }
    });
</script>
<script>
   function aaa (obj) {
       var url = $(obj).parent().attr('code');

       window.open('index1.html?'+ url);





   }
</script>
</body>

</html>
