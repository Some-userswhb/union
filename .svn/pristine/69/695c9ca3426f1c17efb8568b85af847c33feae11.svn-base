<body class="gray-bg">
<div class="wrapper wrapper-content animated fadeInRight" id="indexVue">
    <div class="row">
        <div class="col-sm-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title ourclour">
                    <h5><i class="fa fa-gears"></i>&nbsp;工会管理</h5>
                </div>
                <div class="ibox-content clear shadow">
                    <div class="col-sm-2 no-padding wid135 range1" style="display: none" id="noticeManager">
                        <a href="index1.html" target="_blank" class=“J_menuItem”>
                            <div class="showbox">
                                <i class="fa fa-volume-up"></i>
                                <p>通知管理</p>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-2 no-padding wid135 range2" style="display: none" id="realNameManager">
                        <a href="realnameRecord.html" class=“J_menuItem”>
                            <div class="showbox">
                                <i class="fa fa-address-card"></i>
                                <p>用户实名管理</p>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-2 no-padding wid135 range3" style="display: none" id="organManager">
                        <a href="javascript:void(0);" onclick="aaa(this)"  class=“J_menuItem”>
                            <div class="showbox">
                                <i class="fa fa-sitemap"></i>
                                <p>组织管理</p>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-2 no-padding wid135 range4" style="display: none" id="unitManager">
                        <a href="enterprisesUnit_manage.html" target="_top">
                            <div class="showbox">
                                <i class="fa fa-building"></i>
                                <p>企事业管理</p>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-2 no-padding wid135 range5" style="display: none" id="memManager">
                        <a href="a10_manager.html" class=“J_menuItem”>
                            <div class="showbox">
                                <i class="fa fa-vimeo"></i>
                                <p>会员管理</p>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-2 no-padding wid135 range6" style="display: none" id="personManager">
                        <a href="organization_manage.html" class=“J_menuItem”>
                            <div class="showbox">
                                <i class="fa fa-users"></i>
                                <p>工会人事管理</p>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-2 no-padding wid135 range7" style="display: none" id="platManager">
                        <a href="#" class=“J_menuItem”>
                            <div class="showbox">
                                <i class="fa fa-desktop"></i>
                                <p>平台内容管理</p>
                            </div>
                        </a>
                    </div>
                    <div class="col-sm-2 no-padding wid135 range8" style="display: none" id="dataManager">
                        <a href="#" class=“J_menuItem”>
                            <div class="showbox">
                                <i class="fa fa-bar-chart"></i>
                                <p>数据统计中心</p>
                            </div>
                        </a>
                    </div>
                </div>
