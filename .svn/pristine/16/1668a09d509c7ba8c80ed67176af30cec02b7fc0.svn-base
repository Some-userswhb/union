<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <title>添智·智慧工会云服务平台</title>
    <meta name="keywords">
    <meta name="description">

    <link rel="shortcut icon" href="favicon.ico">
    <link href="css/bootstrap.min.css?v=3.3.6" rel="stylesheet">
    <link href="css/font-awesome.css?v=4.4.0" rel="stylesheet">
    <link href="css/animate.css" rel="stylesheet">
    <link href="css/style.css?v=4.1.0" rel="stylesheet">

</head>
<style>
    .col-sm-8 input.form-control, .col-sm-8 select.form-control {
        width: 50%;
        display: inline-block;
    }

    .help-block.m-b-none {
        display: inline;
        margin-left: 10px;
    }

    .form-group {
        margin-bottom: 20px;
    }
</style>
<body class="gray-bg">
<div class="row">
    <div class="col-sm-12">
        <div class="ibox float-e-margins">
            <div class="ibox-content">
                <form class="form-horizontal m-t" id="signupForm">
                    <div class="form-group">
                        <label class="col-sm-3 control-label">上级工会：</label>
                        <div class="col-sm-8">
                            <input id="firstname" name="firstname" class="form-control" type="text" value="山西省总工会"
                                   disabled>
                            <span class="help-block m-b-none"><i class="fa fa-info-circle"></i> 上级工会</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">工会分类：</label>
                        <div class="col-sm-8">
                            <input id="lastname" name="lastname" class="form-control"
                                   type="text" aria-required="true"
                                   aria-invalid="false" class="valid" v-model="age">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">工会类型：</label>
                        <div class="col-sm-8">
                            <input id="username" name="username" class="form-control" type="text" aria-required="true"
                                   aria-invalid="true" class="error">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label" >工会名称：</label>
                        <div class="col-sm-8">
                            <input id="password" name="password" class="form-control" type="text" v-model="name">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">工会编号：</label>
                        <div class="col-sm-8">
                            <input id="email" name="email" class="form-control" type="number">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">建会时间：</label>
                        <div class="col-sm-8">
                            <input id="confirm_password" name="confirm_password" class="form-control" type="text">
                            <span class="help-block m-b-none">
                                <i class="fa fa-info-circle"></i>
                                请输入建会时间</span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">行政区划：</label>
                        <div class="col-sm-8">
                            <select class="form-control" id='area' name="area">
                                <option>140000</option>
                                <option>140001</option>
                                <option>140002</option>
                                <option>140003</option>
                            </select>
                            <span class="help-block m-b-none"><i class="fa fa-info-circle"></i> 请输入建会时间</span>
                        </div>

                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">工会地址：</label>
                        <div class="col-sm-8">
                            <input id="address" name="address" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">单位类型：</label>
                        <div class="col-sm-8">
                            <select class="form-control" id='copsty' name="copsty">
                                <option>组织工会</option>
                                <option>新型工会</option>
                                <option>新建工会</option>
                                <option>工会管理</option>
                            </select>
                            <span class="help-block m-b-none"><i class="fa fa-info-circle"></i> 单位类型</span>
                        </div>

                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">社会保险缴纳：</label>
                        <div class="col-sm-8">
                            <select class="form-control" id='fee' name="fee">
                                <option>工会管理</option>
                                <option>组织工会</option>
                                <option>新型工会</option>
                                <option>新建工会</option>

                            </select>
                            <span class="help-block m-b-none"><i class="fa fa-info-circle"></i> 社会保险缴纳</span>
                        </div>

                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">职工人数：</label>
                        <div class="col-sm-8">
                            <input id="peples" name="peples" class="form-control" type="text">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">负责人：</label>
                        <div class="col-sm-8">
                            <input id="coper" name="coper" class="form-control" type="text" aria-required="true"
                                   aria-invalid="true" class="error">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">负责人电话：</label>
                        <div class="col-sm-8">
                            <input id="emailq" name="emailq" class="form-control" type="number">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">社会统一信用代码：</label>
                        <div class="col-sm-8">
                            <input id="emailqq" name="emailqq" class="form-control" type="text">
                            <button class="btn btn-primary" type="button">添加</button>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">建会单位：</label>
                        <div class="col-sm-8">
                            <div class="table-responsive">
                                <table class="table table-striped">
                                    <thead>
                                    <tr>
                                        <th>单位名称</th>
                                        <th>社会统一信用代码</th>
                                        <th>单位类型</th>
                                        <th>单位行业</th>
                                        <th>操作</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td>山西普罗迪克基层工会</td>
                                        <td>001111121211015</td>
                                        <td>国有企业</td>
                                        <td>IT高科技</td>
                                        <td>
                                            <i class="fa fa-trash"></i>
                                        </td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-3 control-label">工会备注：</label>
                        <div class="col-sm-8">
                            <textarea id="ccomment" name="comment" class="form-control" required=""
                                      aria-required="true" style="resize: none"></textarea>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-sm-8 col-sm-offset-3">
                            <button class="btn btn-primary" type="submit" @click="goto">保存</button>
                            <button class="btn btn-default" type="button">关闭</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- 全局js -->
<script src="js/jquery.min.js?v=2.1.4"></script>
<script src="js/bootstrap.min.js?v=3.3.6"></script>
<!--vue axios-->
<script src="js/vue2.5.22.js"></script>
<script src="js/ixios.0.18.0.js"></script>
<script>
    new Vue({
        el:'#signupForm',
        data:{
            list:[],
            name:'山西省',
            age:'1',

        },
        methods:{
            goto:function () {
                axios({
                    method: 'post',
                    url: "http://112.35.92.177:90/passLaborweb/api/laborun/union/establish",
                    data: {
                        "laborunLevel":"",
                        "laborunName":"",
                        "laborunCode":"",
                        "parentLaborunCode":"",
                        "laborunKind":"",
                        "laborunType":"",
                        "administratCode":"",
                        "establishTime": "",
                        "laborunAddr":"",
                        "unitType":"",
                        "isTax":"",
                        "principal":"",
                        "principalPhone":""
                    }
                }).then((res) => {
                    /*console.log(JSON.stringify(res.data))*/
                    console.log(res.data)
                    console.log(res.data.data)
                    console.log(this.name)
                });
                console.log(this.name)

            }
        },
        mounted(){

        }
    })
</script>

<!-- 自定义js -->
<script src="js/content.js?v=1.0.0"></script>



<!-- jQuery Validation plugin javascript-->
<!--<script src="js/plugins/validate/jquery.validate.min.js"></script>-->
<!--<script src="js/plugins/validate/messages_zh.min.js"></script>-->

<!--<script src="js/demo/form-validate-demo.js"></script>-->

</body>

</html>
